{
  "name": "next_latest_full_project-setup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_APP_ENV=dev next dev",
    "start": "next start",
    "build:uat": "env-cmd -f enviroment/.env.uat next build",
    "dev:uat": "env-cmd -f enviroment/.env.uat next dev",
    "dev:local": "env-cmd -f enviroment/.env.local next dev",
    "dev:production": "env-cmd -f enviroment/.env.production next dev",
    "build": "pnpm run build:clear && next build",
    "build:test": "pnpm run build:clear && cross-env NEXT_PUBLIC_APP_ENV=test next build",
    "build:clear": "shx rm -rf ./.next",
    "e2e": "playwright",
    "e2e:test": "pnpm run e2e:clear && pnpm run build:test && playwright test",
    "e2e:codegen": "pnpm run e2e:clear && playwright codegen",
    "e2e:debug": "pnpm run e2e:clear && pnpm run build:test && playwright --debug",
    "e2e:clear": "shx rm -rf ./e2e/reports && shx rm -rf ./e2e/results",
    "generate": "plop --plopfile ./templates/plop/plopfile.js",
    "apigen": "pnpm run apigen:prepare && pnpm run apigen:process:account  && pnpm run apigen:process:aircraft && pnpm run apigen:process:flightplan  && pnpm apigen:modify ",
    "apigen:flightplan": "pnpm run apigen:prepare && pnpm run apigen:process:flightplan  && pnpm apigen:modify",
    "apigen:prepare": "shx rm -rf ./src/services/rest-api/generated/**",
    "apigen:process:account": "openapi-generator-cli generate --config ./api-generator-account.yaml --skip-validate-spec",
    "apigen:process:aircraft": "openapi-generator-cli generate --config ./api-generator-airCraft.yaml --skip-validate-spec",
    "apigen:process:flightplan": "openapi-generator-cli generate --config ./api-generator-flightPlan.yaml --skip-validate-spec",
    "apigen:modify": "node modifyFlattenedQueryParams"
  },
  "dependencies": {
    "@ant-design/icons": "5.1.4",
    "@tanstack/react-query": "4.28.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/typescript-estree": "5.59.8",
    "antd": "5.2.1",
    "autoprefixer": "10.4.14",
    "axios": "1.3.2",
    "dayjs": "1.11.7",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.12",
    "next-seo": "6.0.0",
    "next-sitemap": "4.1.3",
    "next-themes": "0.2.1",
    "postcss": "8.4.24",
    "prettier": "2.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "react-redux": "^8.1.2",
    "react-toastify": "^9.1.3",
    "sharp": "0.32.1",
    "tailwindcss": "3.3.2",
    "touch-cli": "^0.0.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@playwright/test": "1.30.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "barrelsby": "2.5.1",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "plop": "3.1.2",
    "shx": "0.3.4"
  }
}
